class Solution:
    def twoSum(self, numbers: List[int], target: int) -> List[int]:

        #hash map
        # insert a value into the hashmap, value -> index
        hash_map = {}


        # index value, we check for sum value in hashmap
        for i in range(len(numbers)):
            diff = target - numbers[i]
            if diff not in hash_map:
                # if no completion sum value, then we add value to hashmap
                hash_map[numbers[i]] = i
            else:
                return [hash_map[diff]+1,i+1]

# Beats 87% of others
# 3ms - fast
# constant time
